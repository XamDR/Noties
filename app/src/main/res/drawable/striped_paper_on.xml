<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape android:shape="rectangle">
			<size
				android:height="60dp"
				android:width="40dp"/>
			<solid android:color="?attr/colorSurface"/>
