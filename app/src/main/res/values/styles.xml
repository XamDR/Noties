<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
	<style name="ThemeOverlay.AppTheme.TextInputEditText.Outlined" parent="">
		<item name="colorControlActivated">?attr/colorPrimary</item>
	</style>

	<style name="ThemeOverlay.MyTheme" parent="ThemeOverlay.AppCompat.DayNight.ActionBar">
		<item name="android:background" tools:ignore="PrivateResource">?attr/colorSurfaceVariant</item>
	</style>

	<style name="ShapeAppearanceOverlay.Dialog.Rounded" parent="">
		<item name="cornerFamily">rounded</item>
		<item name="cornerSize">16dp</item>
	</style>

	<style name="MyThemeOverlay.MaterialAlertDialog" parent="@style/ThemeOverlay.MaterialComponents.MaterialAlertDialog">
		<item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay.Dialog.Rounded</item>
		<item name="android:background" tools:ignore="PrivateResource">?attr/colorSurfaceVariant</item>
	</style>

	<style name="AppTheme.AlertDialogTheme" parent="ThemeOverlay.MaterialComponents.MaterialAlertDialog">
		<item name="dialogCornerRadius">16dp</item>
		<item name="android:background" tools:ignore="PrivateResource">?attr/colorSurfaceVariant</item>
	</style>

	<style name="ScaleAnimationDialog">
		<item name="android:windowEnterAnimation">@anim/anim_in</item>
		<item name="android:windowExitAnimation">@anim/anim_out</item>
	</style>

	<style name="MyCustomNavTextStyle">
		<item name="android:fontFamily">@font/jetbrains_mono_medium</item>
	</style>
</resources>